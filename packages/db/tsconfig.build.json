{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "incremental": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "rootDir": "src",
    "outDir": "dist",
    "emitDeclarationOnly": false,
    "declaration": true,
    "tsBuildInfoFile": "dist/.tsbuildinfo"
  },
  "include": ["src"]
}
