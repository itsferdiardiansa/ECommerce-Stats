{
  "name": "@rufieltics/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "pnpm run generate:main && pnpm run generate:analytics",
    "generate:main": "prisma generate --schema=prisma/schema.prisma",
    "generate:analytics": "prisma generate --config prisma.analytics.config.ts",
    "migrate:dev": "pnpm run migrate:dev:main && pnpm run migrate:dev:analytics",
    "migrate:dev:main": "prisma migrate dev --schema=prisma/schema.prisma",
    "migrate:dev:analytics": "prisma migrate dev --config prisma.analytics.config.ts",
    "migrate:deploy": "pnpm run migrate:deploy:main && pnpm run migrate:deploy:analytics",
    "migrate:deploy:main": "prisma migrate deploy --schema=prisma/schema.prisma",
    "migrate:deploy:analytics": "prisma migrate deploy --config prisma.analytics.config.ts",
    "studio": "prisma studio --schema=prisma/schema.prisma",
    "studio:analytics": "prisma studio --config prisma.analytics.config.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json && node scripts/copyPrismaGenerated.mjs",
    "postinstall": "pnpm run generate && pnpm run build"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "7.3.0",
    "dotenv": "^16.0.0",
    "pg": "^8.17.2",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "prisma": "^7.3.0",
    "tsc-alias": "^1.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./prisma/generated/index.js": "./prisma/generated/index.js"
  }
}
