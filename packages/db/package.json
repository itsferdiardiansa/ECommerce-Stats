{
  "name": "@rufieltics/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "migrate:dev": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "generate": "prisma generate --schema=prisma/schema.prisma",
    "db:studio": "prisma studio",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "7.3.0",
    "dotenv": "^16.0.0",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "prisma": "^7.3.0",
    "typescript": "^5.9.0"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./prisma/generated/index.js"
    }
  }
}
